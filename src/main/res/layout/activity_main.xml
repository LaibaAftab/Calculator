<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="19dp">

    <Button
        android:id="@+id/button_3"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginStart="1dp"
        android:backgroundTint="#478C85"
        android:onClick="three"
        android:text="@string/three"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_2"
        app:layout_constraintTop_toBottomOf="@id/button_6"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_1"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:backgroundTint="#478C85"
        android:onClick="one"
        android:text="@string/one"
        android:textColorHighlight="#FDF7F7"
        android:textSize="34sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_4"
        tools:ignore="MissingConstraints,TextContrastCheck" />


    <Button
        android:id="@+id/button_4"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:backgroundTint="#478C85"
        android:onClick="four"
        android:text="@string/four"
        android:textSize="34sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_7"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_5"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="five"
        android:text="@string/five"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_4"
        app:layout_constraintTop_toBottomOf="@id/button_8"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />


    <Button
        android:id="@+id/button_2"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="two"
        android:text="@string/two"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_1"
        app:layout_constraintTop_toBottomOf="@id/button_5"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_6"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="six"
        android:text="@string/six"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_5"
        app:layout_constraintTop_toBottomOf="@id/button_9"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_7"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#478C85"
        android:onClick="seven"
        android:text="@string/seven"
        android:textSize="34sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_eq"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_8"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="eight"
        android:text="@string/eight"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_7"
        app:layout_constraintTop_toBottomOf="@id/button_eq"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_9"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="nine"
        android:text="@string/nine"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_8"
        app:layout_constraintTop_toBottomOf="@id/button_clear"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck,TextContrastCheck" />

    <Button
        android:id="@+id/button_plus"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="plus"
        android:text="@string/plus"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@id/button_9"
        app:layout_constraintTop_toBottomOf="@id/button_clear"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button_zero"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="zero"
        android:text="@string/zero"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_percent"
        app:layout_constraintTop_toBottomOf="@id/button_2"

        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_percent"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:backgroundTint="#478C85"
        android:onClick="percent"
        android:text="%"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_1"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button_minus"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="minus"
        android:text="@string/minus"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@id/button_6"
        app:layout_constraintTop_toBottomOf="@id/button_plus"
        tools:ignore="MissingConstraints,TextContrastCheck" />


    <Button
        android:id="@+id/button_mul"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="mul"
        android:text="@string/mul"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_zero"
        app:layout_constraintTop_toBottomOf="@id/button_3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_div"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="divide"
        android:text="@string/div"
        android:textSize="30sp"
        app:layout_constraintLeft_toRightOf="@id/button_3"
        app:layout_constraintTop_toBottomOf="@id/button_minus"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_clear"
        android:layout_width="169dp"
        android:layout_height="80dp"
        android:layout_marginRight="10dp"
        android:backgroundTint="#478C85"
        android:onClick="clear"
        android:text="Clear"
        android:textSize="20sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button_eq"
        android:layout_width="169dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:backgroundTint="#478C85"
        android:onClick="equal"
        android:text="@string/equal"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView" />

    <Button
        android:id="@+id/button_back"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_marginLeft="1dp"
        android:backgroundTint="#478C85"
        android:onClick="backspace"
        android:text="back"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@id/button_mul"
        app:layout_constraintTop_toBottomOf="@id/button_div"

        />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:textSize="34sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>